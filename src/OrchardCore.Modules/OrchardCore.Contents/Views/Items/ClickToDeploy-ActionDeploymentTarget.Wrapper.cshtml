@Model.Metadata.ChildContent

@* Inject the modal window only once per page *@
@if (!Context.Items.ContainsKey("ClickToDeploy_Modal__ActionDeploymentTarget"))
{
    Context.Items["ClickToDeploy_Modal__ActionDeploymentTarget"] = new object();
    <zone name="Footer">
        @await DisplayAsync(await New.ClickToDeploy_Modal__ActionDeploymentTarget())
    </zone>
}
